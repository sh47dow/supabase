{
  "name": "@supabase/supabase-js",
  "version": "0.1.24",
  "description": "Supabase Realtime API",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "mocha --require babel-core/register --require babel-polyfill test/unit/**/*.js",
    "test:integration:only": "mocha --require babel-core/register --require babel-polyfill test/integration/**/*.js",
    "test:integration": "docker-compose -f ./test/integration/docker-compose.yml up -d && sleep 10 && mocha --require babel-core/register --require babel-polyfill test/integration/**/*.js ; docker-compose -f ./test/integration/docker-compose.yml down",
    "test:prod": "cross-env BABEL_ENV=production npm run test",
    "test:watch": "npm test -- --watch",
    "test:examples": "node examples/",
    "cover": "nyc --check-coverage && npm test",
    "build": "cross-env BABEL_ENV=production babel src --out-dir lib",
    "prepublish": "npm run clean && npm run build",
    "deploy": "npm publish --access=public"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": "https://github.com/supabase/supabase",
  "keywords": [
    "supabase",
    "realtime",
    "postgrest",
    "javascript"
  ],
  "author": "Supabase",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/supabase/supabase/issues"
  },
  "homepage": "https://supabase.io",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.3.0",
    "chai": "^4.1.2",
    "cross-env": "^5.2.1",
    "jest-websocket-mock": "^2.0.1",
    "mocha": "^7.1.0",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "@supabase/doctest-js": "^0.1.0",
    "@supabase/postgrest-js": "^0.8.0",
    "@supabase/realtime-js": "^0.1.6",
    "superagent": "^5.2.1"
  }
}
